    "@types/node": "^22.15.29",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^9.33.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.5",
    "prettier": "^3.5.3",
